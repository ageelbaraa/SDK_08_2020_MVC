@model IEnumerable<WebApplication8.Models.Customer>

@{
    ViewBag.Title = "IndexLest";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>IndexLest</h2>
@if (!Model.Any())
{
    <text>We Do not Have Any Recourd Sorry</text>
}
else
{
    <table class="table table-bordered table-hover ">
        <thead>
        <tr>
            <th>
                Customer
            </th>
            <th>
               DiscountRate
            </th>
        </tr>
        </thead>
        <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(customer.Name, "Detalis", "Product", new { id = customer.Id }, null)
                </td>
                <td>
                    @customer.MemberShipTypes.Name
                </td>
            </tr>
        }

        </tbody>
    </table>
   
}



